#include <stdio.h>

int main() {
    int n, key, i;
    int found = 0; // Flag to check if the element is found

    // Prompt user for array size and read input
    // The prompt is implicit per the input format description, the program directly reads the input.
    scanf("%d", &n);

    int arr[n]; // Declare array of size n

    // Read elements of the array
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Read the element to search for
    scanf("%d", &key);

    // Perform linear search
    for (i = 0; i < n; i++) {
        if (arr[i] == key) {
            found = 1;
            break; // Element found, exit the loop
        }
    }

    // Display the result
    if (found) {
        printf("found at position %d\n", i); // i holds the 0-indexed position
    } else {
        printf("%d not found\n", key);
    }

    return 0;
}

